{
  "name": "rxn-renderer",
  "version": "0.2.0",
  "description": "RXN file renderer",
  "main": "lib/index.js",
  "module": "src/index.js",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "build": "cheminfo-build --entry src/index.js --root RXNRenderer",
    "eslint": "eslint src",
    "eslint-fix": "npm run eslint -- --fix",
    "prettier": "prettier --check src",
    "prettier-write": "prettier --write src",
    "prepack": "rollup -c",
    "test": "npm run test-only && npm run eslint && npm run prettier",
    "test-only": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cheminfo/rxn-renderer.git"
  },
  "keywords": [],
  "author": "Luc Patiny",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cheminfo/rxn-renderer/issues"
  },
  "homepage": "https://github.com/cheminfo/rxn-renderer#readme",
  "jest": {
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.20.11",
    "cheminfo-build": "^1.1.11",
    "eslint": "^8.31.0",
    "eslint-config-cheminfo": "^8.1.3",
    "jest": "^29.3.1",
    "npm-run-all": "^4.1.5",
    "openchemlib": "^8.1.0",
    "prettier": "^2.8.2",
    "rollup": "^3.9.1"
  },
  "dependencies": {
    "rxn-parser": "^0.1.0"
  }
