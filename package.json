{
  "name": "rxn-renderer",
  "version": "1.0.1",
  "description": "RXN file renderer",
  "main": "lib/index.js",
  "module": "src/index.js",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "build": "cheminfo-build --entry src/index.js --root RXNRenderer",
    "eslint": "eslint src",
    "eslint-fix": "npm run eslint -- --fix",
    "prettier": "prettier --check src",
    "prettier-write": "prettier --write src",
    "prepack": "rollup -c",
    "test": "npm run test-only && npm run eslint && npm run prettier",
    "test-only": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cheminfo/rxn-renderer.git"
  },
  "keywords": [],
  "author": "Luc Patiny",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cheminfo/rxn-renderer/issues"
  },
  "homepage": "https://github.com/cheminfo/rxn-renderer#readme",
  "jest": {
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.24.8",
    "@types/jest": "^29.5.12",
    "cheminfo-build": "^1.2.0",
    "eslint": "^8.57.0",
    "eslint-config-cheminfo": "^10.0.0",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "openchemlib": "^8.14.0",
    "prettier": "^3.3.3",
    "rollup": "^4.19.0"
  },
  "dependencies": {
    "rxn-parser": "^0.1.2"
  }
